{
	"$schema": "https://playground.wordpress.net/blueprint-schema.json",
	"login": true,
	"features": {
		"networking": true
	},
	"preferredVersions": {
		"php": "8.0",
		"wp": "latest"
	},
	"steps": [
		{
			"step": "updateUserMeta",
			"meta": {
				"admin_color": "modern",
				"show_welcome_panel": 0
			},
			"userId": 1
		},
		{
			"step": "setSiteOptions",
			"options": {
				"blogname": "Kindling Demo"
			}
		},
		{
			"step": "installTheme",
			"themeData": {
				"resource": "url",
				"url": "https://github-proxy.com/proxy/?repo=matchboxdesigngroup/kindling&branch=4.0.0-alpha"
			},
			"options": {
				"activate": true
			}
		},
		{
			"step": "importWxr",
			"file": {
				"resource": "url",
				"url": "https://github.com/wpaccessibility/a11y-theme-unit-test/blob/master/a11y-theme-unit-test-data.xml"
			}
		}
	]
}