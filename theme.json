              "fontFamily": "Proxima Nova",
              "fontStretch": "normal",
              "fontStyle": "normal",
              "fontWeight": "900",
              "src": [
                "file:./assets/fonts/proxima-nova/proxima-nova-black.otf"
              ]
            },
            {
              "fontFamily": "Proxima Nova",
              "fontStretch": "normal",
              "fontStyle": "italic",
              "fontWeight": "900",
              "src": [
                "file:./assets/fonts/proxima-nova/proxima-nova-black-it.otf"
              ]
            }
          ],
          "fontFamily": "\"Proxima Nova\", sans-serif",
          "name": "Proxima Nova",
          "slug": "proxima-nova"
        },
        {
          "fontFace": [
            {
              "fontFamily": "Din Pro",
              "fontStretch": "normal",
              "fontStyle": "normal",
              "fontWeight": "400",
              "src": [
