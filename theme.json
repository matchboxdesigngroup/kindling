              "src": [
                "file:./assets/fonts/proxima-nova/proxima-nova-semibold-it.otf"
              ]
            },
            {
              "fontFamily": "Proxima Nova",
              "fontStretch": "normal",
              "fontStyle": "normal",
              "fontWeight": "700",
              "src": [
                "file:./assets/fonts/proxima-nova/proxima-nova-bold.otf"
              ]
            },
            {
              "fontFamily": "Proxima Nova",
