    },
    "color": {
      "palette": [
        {
          "color": "#ffffff",
          "name": "White",
          "slug": "white"
        },
        {
          "color": "#000000",
          "name": "Black",
          "slug": "black"
        },
        {
          "color": "#4d4d4f",
          "name": "Grey",
          "slug": "grey"
        },
        {
          "color": "#f6f6f6",
          "name": "Light Grey",
          "slug": "light-grey"
        },
        {
          "color": "#667080",
          "name": "Medium Grey",
          "slug": "medium-grey"
        },
        {
          "color": "#333333",
          "name": "Dark Grey",
          "slug": "dark-grey"
        },
        {
          "color": "#003da7",
          "name": "Blue",
          "slug": "blue"
        },
        {
          "color": "#002668",
          "name": "Dark Blue",
          "slug": "dark-blue"
        },
        {
          "color": "#f5873c",
          "name": "Orange",
          "slug": "orange"
        },
        {
          "color": "#3e6533",
          "name": "Green",
          "slug": "green"
        },
        {
          "color": "#375d2c",
          "name": "Dark Green",
          "slug": "dark-green"
        }
      ]
    },
    "layout": {
      "contentSize": "1200px",
      "wideSize": "1440px"
